    "@react-navigation/native-stack": "^6.11.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react-native": "~6.14.0",
    "@shopify/flash-list": "1.7.6",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
