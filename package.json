{
  "name": "smile-platform-mobile",
  "version": "5.0.13",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "start:prod": "APP_ENVIRONMENT=production expo start --dev-client",
    "start:staging": "APP_ENVIRONMENT=staging expo start --dev-client",
    "start:expo": "expo start",
    "start:expo-prod": "APP_ENVIRONMENT=production expo start",
    "start:expo-staging": "APP_ENVIRONMENT=staging expo start",
    "android": "expo run:android",
    "android:prod": "APP_ENVIRONMENT=production expo run:android",
    "android:staging": "APP_ENVIRONMENT=staging expo run:android",
    "ios": "expo run:ios",
    "ios:prod": "APP_ENVIRONMENT=production expo run:ios",
    "ios:staging": "APP_ENVIRONMENT=staging expo run:ios",
    "test": "jest --config jest.config.js --coverage",
    "prepare": "husky install",
    "lint": "eslint ./src --quiet",
    "lint:fix": "eslint ./src --fix --quiet",
    "lint:pre-commit": "eslint --ignore-path ./.eslintignore $(git diff --staged --name-only | grep -E '.ts$|.tsx$' | xargs)",
    "lint:pre-commit--fix": "eslint --ignore-path ./.eslintignore $(git diff --staged --name-only | grep -E '.ts$|.tsx$' | xargs) --fix",
    "format:pre-commit": "prettier --check $(git diff --staged --name-only | grep -E '.ts$|.tsx$|.scss$' | xargs)"
  },
  "dependencies": {
